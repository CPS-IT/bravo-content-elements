#+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
# Textmedia
# Custom configuration textmedia element.
# Enable image field in text media element
# CType: textmedia
tt_content.textmedia {
  dataProcessing {
    30 = files
    30 {
      references {
        fieldName = image
      }
      as = previewImage
    }
  }
}
